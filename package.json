{
  "name": "@xpack-dev-tools/clang",
  "version": "12.0.1-1",
  "description": "A binary xPack with the LLVM clang executables",
  "main": "",
  "scripts": {
    "npm-install": "npm install",
    "pack": "npm pack",
    "git-log": "git log --pretty='%cd * %h %s' --date=short",
    "test": "echo \"Error: no test specified\" && exit 1",
    "liquidjs": "liquidjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xpack-dev-tools/clang-xpack.git/"
  },
  "bugs": {
    "url": "https://github.com/xpack-dev-tools/clang-xpack/issues/"
  },
  "homepage": "https://github.com/xpack-dev-tools/clang-xpack/#readme",
  "keywords": [
    "xpack",
    "gnu",
    "clang",
    "c",
    "c++"
  ],
  "author": {
    "name": "Liviu Ionescu",
    "email": "ilg@livius.net",
    "url": "https://github.com/ilg-ul/"
  },
  "license": "MIT",
  "xpack": {
    "minimumXpmRequired": "0.10.2",
    "binaries": {
      "destination": "./.content",
      "baseUrl": "https://github.com/xpack-dev-tools/clang-xpack/releases/download/v12.0.1-1",
      "skip": 1,
      "platforms": {
        "darwin-x64": {
          "fileName": "xpack-clang-12.0.1-1-darwin-x64.tar.gz",
          "sha256": "c89264fcb7045421f897c3beb1d70b510aab2ee33d9a9d123a18b73e3f8b72c3"
        },
        "linux-arm64": {
          "fileName": "xpack-clang-12.0.1-1-linux-arm64.tar.gz",
          "sha256": "a5005d804700cf02f88d37bd53cff72ed788487326538f80a35c314fa11d5919"
        },
        "linux-arm": {
          "fileName": "xpack-clang-12.0.1-1-linux-arm.tar.gz",
          "sha256": "3844fb7a70d519722b07bba4c4846ffa7cba8f3b19440746c2db1145b31e7993"
        },
        "linux-ia32": {
          "fileName": "xpack-clang-12.0.1-1-linux-ia32.tar.gz",
          "sha256": "5945a56690f9c2d39f822305a3d2f01429d6a7bc4e745a722bdfaad2623e3022"
        },
        "linux-x64": {
          "fileName": "xpack-clang-12.0.1-1-linux-x64.tar.gz",
          "sha256": "2645701c746a8ba49ddf6b3223acae99bef449ffb23b2a71f8b1364e61ab710c"
        },
        "win32-ia32": {
          "fileName": "xpack-clang-12.0.1-1-win32-ia32.zip",
          "sha256": "82d42ea1d81462bdda1f36b8bb0dc4f00d31764ea559c112b6d5d8d226c03762"
        },
        "win32-x64": {
          "fileName": "xpack-clang-12.0.1-1-win32-x64.zip",
          "sha256": "a6417c60ae33b41844ed84185f017c056a4c35116e8c87566222f56b024035e3"
        }
      }
    },
    "bin": {
      "clang": "./content/bin/clang",
      "clang++": "./content/bin/clang++",
      "clang-check": "./content/bin/clang-check",
      "clang-cl": "./content/bin/clang-cl",
      "clang-cpp": "./content/bin/clang-cpp",
      "clang-doc": "./content/bin/clang-doc",
      "clang-format": "./content/bin/clang-format",
      "clang-offload-bundler": "./content/bin/clang-offload-bundler",
      "clang-offload-wrapper": "./content/bin/clang-offload-wrapper",
      "clang-refactor": "./content/bin/clang-refactor",
      "clang-rename": "./content/bin/clang-rename",
      "clang-scan-deps": "./content/bin/clang-scan-deps",
      "clang-target-wrapper": "./content/bin/clang-target-wrapper",
      "clang-tidy": "./content/bin/clang-tidy",
      "clangd": "./content/bin/clangd",
      "clangd-xpc-test-client": "./content/bin/clangd-xpc-test-client",
      "darwin-debug": "./content/bin/darwin-debug",
      "diagtool": "./content/bin/diagtool",
      "dwp": "./content/bin/dwp",
      "gendef": "./content/bin/gendef",
      "git-clang-format": "./content/bin/git-clang-format",
      "hmaptool": "./content/bin/hmaptool",
      "ld64.lld": "./content/bin/ld64.lld",
      "ld64.lld.darwinnew": "./content/bin/ld64.lld.darwinnew",
      "ld.gold": "./content/bin/ld.gold",
      "ld.lld": "./content/bin/ld.lld",
      "lld": "./content/bin/lld",
      "lld-link": "./content/bin/lld-link",
      "lldb": "./content/bin/lldb",
      "lldb-argdumper": "./content/bin/lldb-argdumper",
      "lldb-instr": "./content/bin/lldb-instr",
      "lldb-server": "./content/bin/lldb-server",
      "lldb-vscode": "./content/bin/lldb-vscode",
      "llvm-addr2line": "./content/bin/llvm-addr2line",
      "llvm-ar": "./content/bin/llvm-ar",
      "llvm-as": "./content/bin/llvm-as",
      "llvm-bitcode-strip": "./content/bin/llvm-bitcode-strip",
      "llvm-config": "./content/bin/llvm-config",
      "llvm-cov": "./content/bin/llvm-cov",
      "llvm-cxxdump": "./content/bin/llvm-cxxdump",
      "llvm-cxxfilt": "./content/bin/llvm-cxxfilt",
      "llvm-cxxmap": "./content/bin/llvm-cxxmap",
      "llvm-diff": "./content/bin/llvm-diff",
      "llvm-dis": "./content/bin/llvm-dis",
      "llvm-dlltool": "./content/bin/llvm-dlltool",
      "llvm-lib": "./content/bin/llvm-lib",
      "llvm-libtool-darwin": "./content/bin/llvm-libtool-darwin",
      "llvm-nm": "./content/bin/llvm-nm",
      "llvm-objcopy": "./content/bin/llvm-objcopy",
      "llvm-objdump": "./content/bin/llvm-objdump",
      "llvm-profdata": "./content/bin/llvm-profdata",
      "llvm-ranlib": "./content/bin/llvm-ranlib",
      "llvm-rc": "./content/bin/llvm-rc",
      "llvm-readelf": "./content/bin/llvm-readelf",
      "llvm-readobj": "./content/bin/llvm-readobj",
      "llvm-size": "./content/bin/llvm-size",
      "llvm-strings": "./content/bin/llvm-strings",
      "llvm-strip": "./content/bin/llvm-strip",
      "llvm-symbolizer": "./content/bin/llvm-symbolizer",
      "llvm-tblgen": "./content/bin/llvm-tblgen",
      "set-xcode-analyzer": "./content/bin/set-xcode-analyzer",
      "split-file": "./content/bin/split-file",
      "wasm-ld": "./content/bin/wasm-ld",
      "widl": "./content/bin/widl"
    },
    "properties": {
      "appName": "LLVM clang",
      "appLCName": "clang",
      "platforms": "all",
      "LIQUIDJS": "liquidjs '{ \"APP_NAME\": \"{{ properties.appName }}\", \"APP_LC_NAME\": \"{{ properties.appLCName }}\", \"platforms\": \"{{ properties.platforms }}\" }'"
    },
    "actions": {
      "generate-workflows": [
        "mkdir -p .github/workflows/",
        "cp scripts/helper/templates/body-github-pre-releases-test.md .github/workflows/",
        "cat scripts/helper/templates/build-liquid.yml | {{ properties.LIQUIDJS }} > .github/workflows/build.yml",
        "cat scripts/helper/templates/test-prime-liquid.yml | {{ properties.LIQUIDJS }} > .github/workflows/test-prime-0.yml",
        "cat scripts/helper/templates/test-docker-linux-intel-liquid.yml | {{ properties.LIQUIDJS }} > .github/workflows/test-docker-linux-intel-0.yml",
        "cat scripts/helper/templates/test-docker-linux-arm-liquid.yml | {{ properties.LIQUIDJS }} > .github/workflows/test-docker-linux-arm-0.yml",
        "cat scripts/helper/templates/publish-release-liquid.yml | {{ properties.LIQUIDJS }} > .github/workflows/publish-release.yml",
        "cat scripts/helper/templates/test-xpm-liquid.yml | {{ properties.LIQUIDJS }} > .github/workflows/test-xpm.yml"
      ],
      "trigger-workflow-build": "bash scripts/helper/trigger-workflow-build.sh",
      "trigger-workflow-test-prime": "bash scripts/helper/tests/trigger-workflow-test-prime.sh",
      "trigger-workflow-test-docker-linux-intel": "bash scripts/helper/tests/trigger-workflow-test-docker-linux-intel.sh",
      "trigger-workflow-test-docker-linux-arm": "bash scripts/helper/tests/trigger-workflow-test-docker-linux-arm.sh",
      "trigger-travis-macos": "bash scripts/helper/tests/trigger-travis-macos.sh",
      "trigger-workflow-publish-release": "bash scripts/helper/trigger-workflow-publish-release.sh",
      "generate-jekyll-post": "bash scripts/helper/generate-jekyll-post.sh",
      "update-package-binaries": "bash scripts/helper/update-package-binaries.sh",
      "trigger-workflow-test-xpm": "bash scripts/helper/tests/trigger-workflow-test-xpm.sh"
    }
  },
  "bundleDependencies": [],
  "devDependencies": {
    "liquidjs": "^9.25.1"
  }
}
